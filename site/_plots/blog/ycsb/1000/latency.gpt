set term pdf truecolor size 2.5,2.0 font "Times-Roman, 12"

set output "ycsb_large.pdf"

set xrange [0:400]
set yrange [0:1]
set xlabel "latency [us]"
set ylabel "CDF"


set style line 1 lc rgb "#4dafa4" lw 4
set style line 2 lc rgb "#ff7f00" lw 4
set style line 3 lc rgb "#e41a1c" lw 4
set style line 4 lc rgb "#377eb8" lw 4
set style line 5 lc rgb "#377eb8" lw 4
set style line 6 lc rgb "#377eb8" lw 4
set style line 7 lc rgb "#377e00" lw 4

set style increment user
set style data lines
set nokey
set xtics 0, 100, 400


plot "crail_dram_get.dat" using 2:($3/47512) smooth bezier ti "Crail - DRAM", \
 "crail_optane_get.dat" using 2:($3/47467) smooth bezier ti "Crail - Optane",\
 "ramcloud_dram_get.dat" using 2:($3/9545) smooth bezier ti "Ramcloud",\
 "as_optane_get.dat" using 2:($3/47473) smooth bezier ti "Airospike - Optane"


